//https://leetcode.com/problems/max-consecutive-ones/
class Solution {
public:
   int findMaxConsecutiveOnes(vector<int>& nums) {
        int n=nums.size();
        int c=0;
        int maxCount=0;

        for(int i=0;i<n;i++)
        {
            if(nums[i]==1)
                c++;
            else
            {
                maxCount=max(maxCount,c);
                c=0;
            }

        }
        return max(maxCount,c);

    }
};

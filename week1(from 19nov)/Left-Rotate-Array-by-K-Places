// https://takeuforward.org/plus/dsa/problems/left-rotate-array
class Solution {
public:
    void rotateArray(vector<int>& nums, int k) {
    int n=nums.size();

    k=k%n;
    vector<int>v;
    for(int i=0;i<k;i++)
    {
      v.push_back(nums[i]);
    }

    for(int i=k;i<n;i++)
    {
        nums[i-k]=nums[i];
    }
    for(int i=n-k;i<n;i++)
    {
        nums[i]=v[i-(n-k)];
    }

}
};
